{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "rootDir": "./",
    "outDir": "wwwroot"
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "wwwroot",
    "typings/globals"
  ]
}
