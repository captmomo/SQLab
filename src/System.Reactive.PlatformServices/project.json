  "frameworks": {
    "dnx451": {
      "compilationOptions": {
        "define": [
          "NO_EVENTARGS_CONSTRAINT",
          "HAS_EDI",
          "HAS_WINRT",
          "HAS_PROGRESS",
          "PREFER_ASYNC",
          "HAS_AWAIT",
          "HAS_APTCA",
          "HAS_DISPATCHER_PRIORITY",
          "HAS_WINFORMS",
          "USE_TIMER_SELF_ROOT"
